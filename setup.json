{
    "name": "yambo",
    "author": "Michael Atambo, Antimo Marrazzo, Prandini Gianluca",
    "author_email": "m.atambo@nano.cnr.it",
    "description": "YAMBO code Plugin for AiiDA",
    "url": "https://bitbucket.org/prandini/yambo_input",
    "license": "TBD",
    "classifiers": [
        "Programming Language :: Python",
        "Environment :: Plugins",
        "Framework :: AiiDa",
        "Framework :: AiiDa",
        "Topic :: Scientific/Engineering :: Physics"
    ],
    "version": "0.1a",
    "install_requires": [
        "aiida-core"
    ],
    "keywords": "yambo aiida workflows",
    "entry_points": {
        "aiida.calculations": [
            "yambo.gw =  aiida_yambo.calculations.yambo:YamboCalculation"
        ],
        "aiida.parsers": [
            "yambo.parser = aiida_yambo.parsers.yambo:YamboParser"
        ],
        "aiida.data":[]
    }
}
